generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider  = "postgresql"
    url       = env("POSTGRES_PRISMA_URL") // uses connection pooling
    directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model Booking {
    id               String   @id @default(cuid())
    user_id          String
    amount_diners    Int
    booking_datetime DateTime
    table_number     Int
}
